%td
  - if match
    - bot1 = Sumobot.find(match.first_bot_id)
    - bot2 = Sumobot.find(match.second_bot_id)
    = render :partial => "tournament_botmatch", :locals => {:match => match, :bot => bot1}
    .bracket_bottom
      = render :partial => "tournament_botmatch", :locals => {:match => match, :bot => bot2}
  - else
    .bracket
    .bracket_bottom
      .bracket
